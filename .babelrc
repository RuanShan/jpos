{
  "comments": false,
  "env": {
    "test": {
      "presets": [
        ["env", {
          "targets": { "node": 6 }
        }],
        "stage-0"
      ],
      "plugins": ["istanbul"]
    },
    "main": {
      "presets": [
        ["env", {
          "targets": { "node": 6 }
        }],
        "stage-0"
      ]
    },
    "renderer": {
      "presets": [
        ["env", {
          "targets": { "browsers": ["Chrome>=60"]  },
          "modules": false
        }],
        "stage-0",
      ]
    },
    "web": {
      "presets": [
        ["env", {
          "targets": { "browsers": ["Chrome>=60"]  },
          "modules": false
        }],
        "stage-0"
      ]
    }
  },
  "plugins": ["transform-runtime","transform-object-rest-spread"]
}
